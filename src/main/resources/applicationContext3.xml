<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="memberDao" class="com.suhee.basic.ch3.dao.MemberDao">
	</bean>
	
	<bean id="memberRegisterService" class="com.suhee.basic.ch3.service.MemberRegisterService">
		<constructor-arg ref="memberDao" />
	</bean>
	
	<bean id="updatePasswordService" class="com.suhee.basic.ch3.service.UpdatePasswordService">
		<constructor-arg ref="memberDao" />
	</bean>
		
</beans>